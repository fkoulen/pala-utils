{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "rules": {
        "indent": [2, "tab"],
        "no-tabs": "off",
        "@typescript-eslint/indent": "off",
        "@typescript-eslint/space-before-function-paren": "off"
    }
}
